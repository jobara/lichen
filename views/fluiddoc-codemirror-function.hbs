<h3><a name="{{name}}">{{name}}</a></h3>
<ul>
<li>Type: {{type}}</li>
<li>File: {{filename}}:{{linenum}}</li>
</ul>
<p><pre>{{{doc}}}</pre></p>
<textarea style="display:none;" id="hidden-source-{{linenum}}">{{{source}}}</textarea>
<pre id="source-{{linenum}}" class="cm-s-default"></pre>
<script>
CodeMirror.runMode(document.getElementById("hidden-source-{{linenum}}").value,
    "application/javascript", document.getElementById("source-{{linenum}}"));
</script>
